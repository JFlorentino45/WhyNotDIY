<form method="post" action="{{ path('app_admin_notifications_delete', {'id': admin_notification.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ admin_notification.id) }}">
    <button  class="btn block w-fit rounded bg-gray-700 text-gray-300 hover:bg-gray-600 hover:text-white px-4 py-1 ml-2">Delete</button>
</form>
